{
  "version": 2,
  "builds": [
    {
      "src": "webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST"],
      "dest": "webhook.py"
    },
    {
      "src": "/api/webhook",
      "methods": ["GET"],
      "dest": "webhook.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "webhook.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 