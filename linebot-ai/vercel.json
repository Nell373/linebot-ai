{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "api/hello.py"
    },
    {
      "src": "/api",
      "dest": "api/hello.py"
    },
    {
      "src": "/api/webhook",
      "dest": "api/webhook.py"
    }
  ]
} 